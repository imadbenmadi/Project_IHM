<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane id="AnchorPane" prefHeight="623.0" prefWidth="657.0" style="-fx-background-color: #f0f0f0;"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project_ihm.EtudiantControler">
    <children>
        <!-- Add other components as needed -->
        <TableView fx:id="etudiantTableView" layoutX="50.0" layoutY="151.0" prefHeight="400.0" prefWidth="557.0">
            <columns>
                <TableColumn fx:id="nomColumn" text="Nom">
                    <cellValueFactory>
                        <PropertyValueFactory property="nom" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="prenomColumn" text="Prenom">
                    <cellValueFactory>
                        <PropertyValueFactory property="prenom" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="numeroEtudiantColumn" text="Numero Etudiant">
                    <cellValueFactory>
                        <PropertyValueFactory property="numeroEtudiant" />
                    </cellValueFactory>
                </TableColumn>

            </columns>
        </TableView>
        <!-- Add other components as needed -->
    </children>
</AnchorPane>
